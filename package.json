{
  "name": "logaxi",
  "version": "0.1.0",
  "description": "A modern and high-performance logging library for NestJS.",
  "author": "Alan SÃ¡ <alkinsret@gmail.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "npm run build -- --watch",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nestjs",
    "logger",
    "log",
    "pino",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alansadev/logaxi.git"
  },
  "bugs": {
    "url": "https://github.com/alansadev/logaxi/issues"
  },
  "homepage": "https://github.com/alansadev/logaxi#readme",
  "dependencies": {
    "pino": "^9.7.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.5",
    "@types/node": "^24.2.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-destructure-depth": "^1.0.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.20.0",
    "vitest": "^3.2.4"
  }
}
